<?xml version="1.0" encoding="UTF-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui" onLoad="Ribbon_Load">
  <ribbon>
    <tabs>
      <tab idMso="TabAddIns">
        <group id="MainGroup"
               label="Favorite Shapes">
          <button id="openSharedFavButton" label="Show Favorite Shapes"
             screentip="Favorite Shapes" onAction="OnOpenSharedFavButton"
             supertip="Opens the Shared FavoritesShapes Overview"/>
        </group>
      </tab>
    </tabs>
  </ribbon>
  <contextMenus>
    <contextMenu idMso="ContextMenuShape">
      <menu id="ShapeFavoritesMenu" label="Favorite Shapes...">
        <button id="SaveFavoriteShapeContextButton" label="Save as Favorite Shape" insertBeforeMso="HyperlinkInsert" onAction="SaveFavoriteShape"/>
      </menu>
    </contextMenu>
  </contextMenus>
</customUI>

